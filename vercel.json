{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "crons": [
    {
      "path": "/api/cron-blob-backup",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/cron-db-backup",
      "schedule": "0 4 * * *"
    },
    {
      "path": "/api/cron-cleanup-backups",
      "schedule": "0 5 * * *"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": false,
      "release-please--*": false
    }
  }
}